{
  "manifest_version": 3,
  "name": "BOJ Addon",
  "version": "2.0.2",
  "description": "BOJ Addon",
  "author": "Namheon Baek",
  "content_scripts": [
    {
      "css": ["src/style.css"],
      "js": ["src/main.tsx"],
      "matches": [
        "https://www.acmicpc.net/problem/*",
        "https://www.acmicpc.net/submit/*",
        "https://*/contest/problem/*"
      ],
      "exclude_matches": [
        "https://www.acmicpc.net/problem/status/*",
        "https://www.acmicpc.net/problem/history/*"
      ]
    }
  ]
}
